	public static void main(String args[])
	{
		SecureRandom rand = new SecureRandom();
		StringBuilder sb= new StringBuilder();
		for (int idx=0; idx<=10; idx++)
		{
			sb.append(rand.nextInt(2));
		}
		System.out.println("Generated: " +sb.toString());
		}
	
	
}


